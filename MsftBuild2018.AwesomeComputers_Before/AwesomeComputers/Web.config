<configuration>
    <appSettings>
        <add key="webpages:Version" value="3.0.0.0" />
        <add key="webpages:Enabled" value="false" />
        <add key="ClientValidationEnabled" value="true" />
        <add key="UnobtrusiveJavaScriptEnabled" value="true" />
        <!--
            Follow the steps in this tutorial: https://docs.microsoft.com/en-us/azure/active-directory-b2c/active-directory-b2c-tutorials-web-app#next-steps
            To set the values below.
        -->
        <add key="ida:Tenant" value="{MYTENANT}.onmicrosoft.com" />
        <add key="ida:ClientId" value="{MY_APPLICATIONID}" />
        <add key="ida:ClientSecret" value="{MY_APPLICATION_SECRET}" />
        <add key="ida:AadInstance" value="https://login.microsoftonline.com/tfp/{0}/{1}/v2.0/.well-known/openid-configuration" />
        <add key="ida:RedirectUri" value="{MY_REDIRECT_URL}" />
        <add key="ida:SignUpSignInPolicyId" value="{MY_SUSI_POLICY_NAME}" />
        <add key="ida:EditProfilePolicyId" value="{MY_EDIT_POLICY_NAME}" />
        <add key="ida:ResetPasswordPolicyId" value="{MY_PW_RESET_POLICY_NAME}" />
    </appSettings>
    <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5.1" />
      </system.Web>
  -->
